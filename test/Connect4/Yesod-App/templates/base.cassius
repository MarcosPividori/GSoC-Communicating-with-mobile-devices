html
    font-family: sans-serif
    background-color: #000000
    font-size: 16px

div#main 
    width: 1000px
    margin: 70px auto 0px auto
    padding: 20px
    background-color: #fff
    height: 520px

div#grid1
    width: 525px
    height: 525px
    display: block
    margin-left: auto
    margin-right: auto

div#info
    width: 400px
    display: block
    margin-left: auto
    margin-right: auto
    text-align: left

a#cancel
    width: 400px
    display: block
    margin: -15px auto 0px auto
    text-align: right

div#authid
    float:right
    margin: -40px 0px 0px 0px
    padding-right:10px
    padding-top:-50px
    color:#0174DF

div#authid a
    color: #fff

div#users
    height: 400px
    width: 800px
    display: block
    margin-left: auto
    margin-right: auto

div#startgame
    height: 400px
    width: 400px
    display: block
    margin-left: auto
    margin-right: auto
    text-align: left
    float: left

div#offline
    display: block
    margin-left: auto
    margin-right: auto

div#messages 
    margin: 0px
    width: 400px
    max-height: 400px
    overflow: auto
    float: right

div#history
    max-height: 200px
    overflow: auto

div#destin 
    max-height: 100px
    overflow: auto

div#footer
    height: 50px
    color: #fff
    padding: 20px 20px 20px 20px
    background-color: #000000
    display: block

div#source
    text-align: center
    float: center

div#source a
    color: #fff

div#deployment
    text-align: right
    float: right
    margin: -20px auto 0px auto

div#deployment a
    color:#0174DF

#alertBox
    position:absolute
    top:200px
    left:200px
    border:solid 1px black
    background-color: #0174DF
    padding: 50px
    visibility: hidden
    color: #000

#alertCloseButton
    position: absolute
    right:0
    top: 0
    background-color: black
    border: solid 1px white
    color: white
    width: 1em
    text-align: center
    color: #fff
    cursor: pointer
